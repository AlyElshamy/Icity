<script>
    function SkillTitle() {
        var SkillList = new Array();
        var inputvalue = document.getElementsByClassName("SkillInput");
        for (var i = 0; i < inputvalue.length; i++) {
            if (document.getElementsByClassName("SkillInput")[i].value != "") {
                SkillList.push(document.getElementsByClassName("SkillInput")[i].value)
            }
        }
        if (SkillList.length > 0) {
            $.ajax({
                type: "POST",
                beforeSend: function (xhr) {
                    xhr.setRequestHeader("XSRF-TOKEN",
                        $('input:hidden[name="__RequestVerificationToken"]').val());
                }
                ,
                url: "/TemplatePages/Profile?handler=FillSkillsList",
                async: false,
                data: JSON.stringify(SkillList),
                dataType: "json",
                contentType: "application/json; charset=utf-8",

                success: function (response) {
                    console.log("get selected rows successfully" + response)
                    document.getElementById("skillsavebtn").textContent = "Saved";
                    document.getElementById("skillsavebtn").style.backgroundColor = "#257260";
                    
                },
                failure: function (response) {
                    alert(response);
                }
            });
           
        }
        else {
            console.log("Nobody has been selected")
        }
    }
    function changeStylebutton(id) {

        document.getElementById(id).textContent = "Save All";
        document.getElementById(id).style.backgroundColor = "#384f95";
    }

    function InterristTitle() {
        var InterestsList = new Array();
        var inputvalue = document.getElementsByClassName("InterrestInput");
        for (var i = 0; i < inputvalue.length; i++) {
            if (document.getElementsByClassName("InterrestInput")[i].value != "") {
